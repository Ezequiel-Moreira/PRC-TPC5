block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-teal
        h1=title
      .w3-container
        p Repositories found in localhost:7200
        for repository in repositories
          p
            a="ID:"
            a=repository.id.value
            a="/ TITLE:"
